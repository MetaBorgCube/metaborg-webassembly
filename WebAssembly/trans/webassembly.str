module webassembly

imports
  
  completion/completion
  WebAssembly/pp
  WebAssembly/outline
  WebAssembly/analysis

rules // Debugging
  
  debug-show-aterm:
    (node, _, _, path, project-path) -> (filename, result)
    with
      filename := <guarantee-extension(|"aterm")> path
    ; result   := node
